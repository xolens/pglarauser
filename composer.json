{
    "name": "xolens/pglarauser",
    "description": "Laravel Users account, User's  Groups and Permission management package.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Xolens",
        "email": "xolens.official@gmail.com"
    }],
    "require-dev": {
        "orchestra/testbench": "~3.4.0"
    },
    "require": {
        "xolens/larautil-contract": "dev-master",
        "xolens/pglarautil": "dev-master",
        "xolens/larauser-contract": "dev-master",
        "xolens/larasetting-contract": "dev-master",
        "xolens/pglarasetting": "dev-master"
    },
    "repositories": [{
        "type": "path",
        "url": "/var/www/repositories/xolens.laravel/local/*"
    }],
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xolens\\PgLarauser\\PgLarauserServiceProvider"
            ]
        }
    }
}
